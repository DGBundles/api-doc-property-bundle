{
  "name": "dawen/api-doc-property-bundle",
  "type": "symfony-bundle",
  "description": "A extension for nelmio api doc bundle to expose properties",
  "keywords": ["nelmio", "api", "doc", "extension", "property", "api-doc-bundle", "bundle", "symfony"],
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Wendlandt",
      "email": "dan.wendlandt@gmail.com"
    }
  ],
  "require": {
    "php": "~7.1.0",
    "symfony/framework-bundle": "~2.3|~3.0|~4.0",
    "nelmio/api-doc-bundle": "~2.11"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "doctrine/common": "^2.8"
  },
  "autoload": {
    "psr-0": { "Dawen\\Bundle\\ApiDocPropertyBundle": "" }
  },
  "target-dir": "Dawen/Bundle/ApiDocPropertyBundle",
  "minimum-stability": "stable"
}
